alias gc='f(){ NAMESPACE=$(echo $1 | cut -d'/' -f1); REPO=$(echo $1 | cut -d'/' -f2); mkdir -p "$NAMESPACE" && cd "$NAMESPACE" && git clone "http://github.com/$1"; }; f'
